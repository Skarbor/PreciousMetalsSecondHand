@model IEnumerable<PrecoiusMetalsSecondHand.Data.Entities.Order>
@{
    ViewData["Title"] = "Home";
}

<h1>Home</h1>
<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">First</th>
        <th scope="col">Last</th>
        <th scope="col">Handle</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.Id</td>
                <td>@order.Description</td>
                <td>@order.CreatedDate</td>
            </tr>
        }
    </tbody>
</table>

